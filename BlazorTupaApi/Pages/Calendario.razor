@page "/calendario"
@using Dominio;
@using Newtonsoft.Json;
@inject HttpClient Http

@eventosJson;

@code {

    private Evento[]? eventos;
    private string? eventosJson;

    private string teste = "Hello Calendário";

    protected override async Task OnInitializedAsync()
    {
        try
        {
            eventos = await Http.GetFromJsonAsync<Evento[]>("sample-data/calendario.json");
            eventosJson = JsonConvert.SerializeObject(eventos);
        }
        catch (Exception ex)
        {            
            Console.WriteLine(ex.Message);
        }
        
    }

}
